<?xml version='1.0' encoding='utf-8'?>
<plugin id="com.urovoscanner" version="1.0.0"
	xmlns="http://apache.org/cordova/ns/plugins/1.0"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<name>CordovaPluginUrovoScanner</name>
	<js-module name="CordovaPluginUrovoScanner" src="www/CordovaPluginUrovoScanner.js">
		<clobbers target="cordova.plugins.CordovaPluginUrovoScanner" />
	</js-module>
	<platform name="android">
		<config-file parent="/*" target="res/xml/config.xml">
			<feature name="CordovaPluginUrovoScanner">
				<param name="android-package" value="com.urovoscanner.CordovaPluginUrovoScanner" />
			</feature>
		</config-file>
    <config-file parent="/*" target="AndroidManifest.xml"></config-file>
		<config-file parent="/*" target="AndroidManifest.xml"></config-file>
		<source-file src="src/android/CordovaPluginUrovoScanner.java" target-dir="src/com/urovoscanner/CordovaPluginUrovoScanner" />
    <source-file src="src/android/libs/platform_sdk_v3.1.0326.jar" target-dir="libs" />
	</platform>
</plugin>
